# Ignore directories generated by Drupal
/web/sites/*/files/
/web/sites/*/private/

# Ignore configuration files that may contain sensitive information
/web/sites/*/settings*.php

# Ignore Drupal core files
/web/core/

# Ignore contributed modules and themes (can be installed via Composer)
/vendor/

# Ignore Composer dependencies
# /composer.json
# /composer.lock
